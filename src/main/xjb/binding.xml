<jxb:bindings
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
        version="2.1">

    <!-- Raise theEnumMemberSizeCap limit -->
    <jxb:bindings >
        <jxb:globalBindings typesafeEnumMaxMembers="2000"/>
    </jxb:bindings>

    <jxb:bindings schemaLocation="../xsd/eis_Common.xsd">
        <jxb:schemaBindings>
            <jxb:package name="org.ici.nmfp.edgar_common"/>
        </jxb:schemaBindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="../xsd/eis_NMFP2_Filer.xsd">
        <jxb:schemaBindings>
            <jxb:package name="org.ici.nmfp"/>
        </jxb:schemaBindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="../xsd/eis_NMFP2_common.xsd">
        <jxb:schemaBindings>
            <jxb:package name="org.ici.nmfp.common"/>
        </jxb:schemaBindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="../xsd/eis_stateCodes.xsd">
        <jxb:schemaBindings>
            <jxb:package name="org.ici.nmfp.states"/>
        </jxb:schemaBindings>
    </jxb:bindings>

</jxb:bindings>